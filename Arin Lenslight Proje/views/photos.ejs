<!--Ortak Headerı İmport Ettim-->
<%- include("partials/_header")%>
   <!-- body -->
   <body class="main-layout inner_page">
      <!-- loader  -->
      <div class="loader_bg">
         <div class="loader"><img src="images/loading.gif" alt="#"/></div>
      </div>
      <!-- end loader -->
     
          <!--Ortak Menü İmport Ettim-->
          <%- include("partials/_menu")%>

      <!-- gallery -->
      <div class="gallery">
         <div class="container_with">
            <div class="row">
               <div class="col-md-12">
                  <div class="titlepage text_align_center">
                     <span>03</span>
                     <h2>Our gallery</h2>
                  </div>
               </div>
            </div>
            <div class="tz-gallery">
                     <div class="row">

                       
                           <!--Verileri Dinamik Şekilde Getirdim-->
                       <% for (photo of photos){%>
                        <div class="col-lg-4 col-md-6 ma_bottom30">
                           <div class="lightbox">
                              <img src="<%=photo.url%>" alt="<%=photo.name%>">
                              <div class="view_main">
                                <div class="pose">
                                <a class="read_more" href="/photos/<%=photo._id%>"><img src="images/ga.png" alt="#"/></a>
                                <h3 style="padding-top: 10px; padding-bottom: 10px">
                                  <!--Photo Name Dinamik Şekilde Getirdim-->
                                <%=photo.name%>
                              </h3>
                              <p style="padding-top: 10px;">
                                  <!--Photo Description Dinamik Şekilde Getirdim-->
                                 <%=photo.description.substring(0,80)+ "..."%> 
                              </p>
                              </div>
                              </div>
                           </div>
                        </div>


                   <% }%>     
                  
               </div>
            </div>
         </div>
      </div>
      <!-- end gallery -->
    <!--Ortak Footer İmport Ettim-->
    <%- include("partials/_footer")%> 